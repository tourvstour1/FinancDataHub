model f_claim_status {
  id               String             @id @default(uuid())
  status_code      String             @unique @db.VarChar
  description      String
  ipd_claim_status ipd_claim_status[]
  opd_claim_status opd_claim_status[]

  @@schema("public")
}

model opd_claim_status {
  id                    String         @id @default(uuid())
  status_code           String
  seq                   String         @db.VarChar(16)
  opd_claim_date        String
  opd_claim_number      String?
  staff_number_claim    String?
  sent_date             String?
  hn                    String?
  fdh_status_message_th String?
  fdh_process_status    String?        @default(dbgenerated("99"))
  fdh_status_message    String?
  status                f_claim_status @relation(fields: [status_code], references: [status_code])

  @@schema("public")
}

model ipd_claim_status {
  id                    String         @id @default(uuid())
  status_code           String
  an                    String         @db.VarChar(16)
  ipd_claim_date        String
  ipd_claim_number      String?
  staff_number_claim    String?
  sent_date             String?
  hn                    String?
  fdh_status_message_th String?
  fdh_process_status    String?        @default(dbgenerated("99"))
  fdh_status_message    String?
  status                f_claim_status @relation(fields: [status_code], references: [status_code])

  @@schema("public")
}

model t_code_error {
  id                     Int     @id @default(autoincrement())
  code_error             String
  code_error_description String?
  main_table             String?
  ref_table              String?
  t_code_error_active    Int     @default(1)
  main_column            String?
  ref_column             String?
  ref_column_condition   String?

  @@schema("public")
}

model item_group {
  item_group_code   String
  item_group_name   String
  item_group_active Int    @default(1)
  id                Int    @id @default(autoincrement())

  @@schema("master_items")
}

model item_main {
  id Int @id @default(autoincrement())

  @@schema("master_items")
}

model lab_main {
  id Int @id @default(autoincrement())

  @@schema("master_items")
}

model drug_free_schedule {
  gpuid        String
  generic_name String
  strength     String
  dosageform   String
  contvalue    String
  contunit     String
  disp_unit    String
  price        String
  id           Int    @id @default(autoincrement())

  @@schema("master_items")
}

model adp_free_schedule {
  item_code String
  item_name String
  unit      String
  type      String
  price     String
  id        Int    @id(map: "adp_free_schedule_copy1_pkey")

  @@schema("master_items")
}

model t_employee {
  id                     String  @id @db.Uuid
  employee_username      String  @db.VarChar
  employee_password      String  @db.VarChar
  employee_firstname     String? @db.VarChar
  employee_lastname      String? @db.VarChar
  employee_hospital_code String  @default(dbgenerated("1")) @db.VarChar
  active                 Int     @default(1) @db.SmallInt

  @@schema("employee")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model adp_nhso {
  item_type          String? @db.VarChar(255)
  item_code          String? @db.VarChar(255)
  item_name          String? @db.VarChar(255)
  item_price         String? @db.VarChar(255)
  item_unit          String? @db.VarChar(255)
  date_start         String? @db.VarChar(255)
  date_end           String? @db.VarChar(255)
  date_update        String? @db.VarChar(255)
  fee_schedule_list1 String? @db.VarChar(255)
  fee_schedule_list2 String? @db.VarChar(255)
  item_type_number   String? @db.VarChar(255)

  @@ignore
  @@schema("master_items")
}

model adp_type {
  adp_catalogy_id   Int     @id
  adp_name          String  @db.VarChar
  adp_type_number   Int[]
  adp_nhso_name     String? @db.VarChar
  adp_hospital_code String? @db.VarChar(255)

  @@schema("master_items")
}

model fdh_claim_status {
  id                    String  @id
  fdh_number_process    String?
  fdh_status_process    String?
  fdh_status_process_th String?

  @@schema("master_items")
}
