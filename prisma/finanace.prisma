generator client {
  provider        = "prisma-client-js"
  output          = "./generated/finanace-client"
  previewFeatures = ["fullTextIndex", "fullTextSearch", "multiSchema", "relationJoins"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_FINANCE_URL")
  schemas  = ["employee", "master_items", "public"]
}

model t_ins {
  id       String  @id @unique @default(uuid())
  hn       String? @db.VarChar(15)
  inscl    String  @db.VarChar(3)
  subtype  String? @db.VarChar(2)
  cid      String? @db.VarChar(16)
  hcode    String  @db.VarChar(5)
  dateexp  String?
  hospmain String  @db.VarChar(5)
  hospsub  String  @db.VarChar(5)
  govcode  String? @db.VarChar(6)
  govname  String? @db.VarChar(255)
  permitno String? @db.VarChar(30)
  docno    String? @db.VarChar(30)
  ownrpid  String? @db.VarChar(13)
  ownname  String? @db.VarChar(255)
  an       String  @db.VarChar(15)
  seq      String  @db.VarChar(15)
  subinscl String? @db.VarChar(2)
  relinscl String? @db.VarChar(1)
  htype    String? @db.VarChar(1)

  @@schema("public")
}

